{
  "name": "rsbuild-plugin-svg-sprites",
  "type": "module",
  "version": "2.0.0",
  "packageManager": "pnpm@9.14.4",
  "description": "Rsbuild Plugin for fast creating SVG sprites & hmr supported.(like vite-plugin-svg-icons)",
  "license": "MIT",
  "repository": "https://github.com/ease-flow/rsbuild-plugins/tree/main/packages/svg-sprites",
  "keywords": [
    "rsbuild",
    "rsbuild-plugin",
    "svg",
    "svg-sprites",
    "svg-icons",
    "vite-plugin-svg-icons"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch"
  },
  "peerDependencies": {
    "@rsbuild/core": "1.x"
  },
  "peerDependenciesMeta": {
    "@rsbuild/core": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}